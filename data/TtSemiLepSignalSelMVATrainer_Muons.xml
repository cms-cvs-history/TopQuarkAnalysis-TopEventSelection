<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MVATrainer>

        <general>
                <option name="id">TtSemiSignalSelMVATrainer</option>
                <option name="trainfiles">train_%1$s%2$s.%3$s</option>
        </general>

        <input id="input">
                <var name="dphiMETlepton"     multiple="false" optional="false"/>
                <var name="dphiMETleadingjet" multiple="false" optional="false"/>
                <var name="ETratiojet5jet4"   multiple="false" optional="false"/>
                <var name="aplanarity"        multiple="false" optional="false"/>
                <var name="sphericity"        multiple="false" optional="false"/>
                <var name="circularity"       multiple="false" optional="false"/>
                <var name="isotropy"          multiple="false" optional="false"/>  
                <var name="sumEt"             multiple="false" optional="false"/>
                <var name="maxEta"            multiple="false" optional="false"/>
                <var name="Et1"               multiple="false" optional="false"/>
                <var name="Et2"               multiple="false" optional="false"/>
                <var name="Et3"               multiple="false" optional="false"/>
                <var name="Et4"               multiple="false" optional="false"/>
                <var name="lepPt"             multiple="false" optional="false"/>    
        </input>

        <processor id="norm" name="ProcNormalize">
                <input>
                        <var source="input" name="dphiMETlepton"/>
                        <var source="input" name="dphiMETleadingjet"/>
                        <var source="input" name="ETratiojet5jet4"/>
                        <var source="input" name="aplanarity"/> 
                        <var source="input" name="sphericity"/> 
                        <var source="input" name="circularity"/> 
                        <var source="input" name="isotropy"/>
                        <var source="input" name="sumEt"/>
                        <var source="input" name="maxEta"/>
                        <var source="input" name="Et1"/>
                        <var source="input" name="Et2"/>
                        <var source="input" name="Et3"/>
                        <var source="input" name="Et4"/>
                        <var source="input" name="lepPt"/>
                </input>
                <config>
                        <pdf/>
                        <pdf/>
                        <pdf/>
                        <pdf/> 
                        <pdf/>
                        <pdf/> 
                        <pdf/>
                        <pdf/>
                        <pdf/>
                        <pdf/>
                        <pdf/> 
                        <pdf/>
                        <pdf/>
                        <pdf/>
                </config>
                <output>
                        <var name="var1"/>
                        <var name="var2"/>
                        <var name="var3"/>
                        <var name="var4"/> 
                        <var name="var5"/> 
                        <var name="var6"/> 
                        <var name="var7"/>
                        <var name="var8"/>
                        <var name="var9"/> 
                        <var name="var10"/> 
                        <var name="var11"/> 
                        <var name="var12"/>
                        <var name="var13"/>
                        <var name="var14"/> 
                </output>
        </processor>

        
        <processor id="rot" name="ProcMatrix">
		<input>
                        <var source="norm" name="var1"/>
                        <var source="norm" name="var2"/>
                        <var source="norm" name="var3"/>
                        <var source="norm" name="var4"/>
                        <var source="norm" name="var5"/>
                        <var source="norm" name="var6"/>
                        <var source="norm" name="var7"/>
                        <var source="norm" name="var8"/>
                        <var source="norm" name="var9"/>
                        <var source="norm" name="var10"/>
                        <var source="norm" name="var11"/>
                        <var source="norm" name="var12"/>
                        <var source="norm" name="var13"/>
                        <var source="norm" name="var14"/>
		</input>
		<config>
			<fill signal="true" background="true"/>
		</config>
		<output>
			<var name="rot1"/>
			<var name="rot2"/>
			<var name="rot3"/>
			<var name="rot4"/>
			<var name="rot5"/>
			<var name="rot6"/>
			<var name="rot7"/>
                        <var name="rot8"/>
                        <var name="rot9"/>
                        <var name="rot10"/>
			<var name="rot11"/>
			<var name="rot12"/>
                        <var name="rot13"/>
                        <var name="rot14"/>
		</output>
	</processor>


	<processor id="likelihood" name="ProcLikelihood">
		<input>
			 <var source="norm" name="var1"/>
			 <var source="norm" name="var2"/>
			 <var source="norm" name="var3"/>
			 <var source="norm" name="var4"/> 
			 <var source="norm" name="var5"/> 
			 <var source="norm" name="var6"/> 
			 <var source="norm" name="var7"/> 
                         <var source="norm" name="var8"/>
                         <var source="norm" name="var9"/>
                         <var source="norm" name="var10"/> 
			 <var source="norm" name="var11"/> 
			 <var source="norm" name="var12"/> 
                         <var source="norm" name="var13"/>
                         <var source="norm" name="var14"/> 
		</input>
		<config>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
			<sigbkg smooth="3"/>
                        <sigbkg smooth="3"/> 
 			<sigbkg smooth="3"/> 
 			<sigbkg smooth="3"/> 
                        <sigbkg smooth="3"/>
                        <sigbkg smooth="3"/>
                        <sigbkg smooth="3"/>
                        <sigbkg smooth="3"/> 
 			<sigbkg smooth="3"/> 
                        <sigbkg smooth="3"/>
                        <sigbkg smooth="3"/>
                        <sigbkg smooth="3"/> 
		</config>
		<output>
			<var name="discriminator"/>
		</output>
	</processor>

        <output>
                <var source="likelihood" name="discriminator"/>
        </output>

</MVATrainer>
